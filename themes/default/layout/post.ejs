<head>
  <%- partial('partial/head') %>
  <title><%- page.title %> | Blog by seamlik</title>
</head>
<body>
  <%- partial('partial/header') %>
  <main class="page-content"><div class="wrapper">
    <article itemscope itemtype="http://schema.org/BlogPosting">
      <header class="post-header">
        <h1 class="post-title p-name" itemprop="name headline"><%= page.title %></h1>
        <p class="post-meta">
          <time class="dt-published" itemprop="datePublished">
            <%= page.date.format('LLL') %>
          </time>
        </p>
      </header>
      <div class="post-content e-content" itemprop="articleBody">
        <%- page.content %>
      </div>
    </article>
  </div></main>
  <%- partial('partial/footer') %>
</body>